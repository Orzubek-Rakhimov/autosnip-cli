{
    "display": "Node LTS",
    "_version": "20.1.0",
    "compilerOptions": {
      "lib": [
        "es2023"
      ],
      "outDir": "dist",
      "module": "node16",
      "target": "es2022",
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node16",
      "resolveJsonModule": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }