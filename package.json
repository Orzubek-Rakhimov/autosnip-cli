{
  "name": "autosnip-cli",
  "version": "0.1.0",
  "main": "./dist/bin.js",
  "bin": {
    "autosnip": "./dist/bin.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "esbuild src/bin.ts --bundle --platform=node --outfile=dist/bin.js --packages=external",
    "link-cli": "npm run build && npm unlink autosnip-cli && npm link",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "react",
    "react-snippet",
    "react-snippet-cli",
    "react-snippet-cli-ts"
  ],
  "author": "",
  "license": "ISC",
  "description": "A CLI to generate React snippets automatically",
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/node": "^22.5.4",
    "esbuild": "^0.23.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^4.0.0",
    "commander": "^12.1.0"
  }
}